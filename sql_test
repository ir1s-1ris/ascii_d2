CREATE TABLE myplayers(
	id SERIAL PRIMARY KEY,
	avatarMd CHARACTER(150),
	lastlogoff int,
	personaname character(30),
	personastate character(15),
	steamaccount32 int,
	steamaccount64 int,
	steamid int,
	timecreated int
	)


CREATE TABLE tableTest(id SERIAL PRIMARY KEY, firstName CHARACTER VARYING(30));